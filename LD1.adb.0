with Text_IO;
use Text_IO;
procedure LD1 is


type Hundred is new Integer range 0..100;

package Hundred_IO is new Integer_IO(Hundred);
use Hundred_IO;

type Measurements is array (Character range <>, Character range <>) of Hundred;


Results : Measurements('a'..'d', 'e'..'i') := (others => (others => 0));

MAX : Integer;
begin
Results('b', 'f') := 50;

for i in Results'First..Results'Last loop
   for j in Results'Range(2) loop
       Put(Results(i,j),3);
   end loop;
   New_Line;
end loop;



for i in Results'First..Results'Last loop
   for j in Results'Range(2) loop

       IF Results(i,j) < Results (i,j)'Succ then
          MAX := Results (i,j)'Suc
       end if
   end loop;
   New_Line;
end loop;

Put(Max,2);

end LD1;